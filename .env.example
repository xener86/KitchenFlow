# KitchenFlow Environment Variables
# Copy this file to .env and fill in the values

# Database - Use your existing PostgreSQL instance
DB_PASSWORD=your_kitchenflow_db_password

# JWT Secret - Generate a secure random string
# Example: openssl rand -base64 32
JWT_SECRET=your_jwt_secret_change_this

# Optional: Gemini API Key (can also be set in the app settings)
GEMINI_API_KEY=
